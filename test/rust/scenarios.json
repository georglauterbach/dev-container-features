{
  "basic": {
    "image": "docker.io/ubuntu:24.04",
    "features": {
      "rust": {
        "rust.rustup.default-toolchain": "stable"
      }
    }
  },
  "advanced": {
    "image": "docker.io/ubuntu:24.04",
    "features": {
      "rust": {
        "rust.install": true,
        "rust.rustup.default-toolchain": "1.81.0",
        "rust.rustup.update-default-toolchain": true,
        "rust.rustup.profile": "default",
        "rust.rustup.additional-targets": "aarch64-unknown-linux-gnu,x86_64-unknown-uefi",
        "rust.rustup.additional-components": "llvm-tools,rustfmt",
        "linker.mold.install": true,
        "linker.mold.version": "2.34.1"
      }
    }
  }
}
